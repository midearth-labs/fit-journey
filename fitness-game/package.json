    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate:custom-ddl": "drizzle-kit generate --custom --name=custom-ddl"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
