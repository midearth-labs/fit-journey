    "@types/react-dom": "^19",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^2.40.7",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
