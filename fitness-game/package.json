    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.3",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.540.0",
    "next": "15.4.7",
